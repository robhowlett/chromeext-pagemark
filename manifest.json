{
  "name": "PageMark",
  "description": "Mark and find your position on the page",
  "version": "0.1",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
      "default_title": "PageMark",
      "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-2.1.3.min.js", "page-mark.js"]
  }],
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "manifest_version": 2
}
